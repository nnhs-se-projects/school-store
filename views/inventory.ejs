<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->

<link rel="stylesheet" href="/css/admin.css">

<main>
  
  <h1>Print</h1>

  <a class="admin-link" href="inventoryprint" id="inventoryprint"><strong>Print</strong></a>

  <h1>Inventory Spreadsheet</h1>

  <table>
    <tr>
      <th> Item </th>
      <th> Quantity </th>
      <th> Sizes </th>
    </tr>
    <% items.forEach(function(item) { %>
    <tr>
      <th> <%= item.name %> </th>
      <th> <%= item.sizes %> </th>
      <th> <%= item.sizes %> </th>
    </tr>
    <% }) %>
  </table>

</main>

</body>

<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->